{
  "name": "workshop-ori-ui",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:rajzik/workshop-ori-ui.git",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@oriflame/config-babel": "^1.0.7",
    "@oriflame/config-eslint": "^1.0.9",
    "@oriflame/config-jest": "^1.0.7",
    "@oriflame/config-prettier": "^1.0.5",
    "@oriflame/config-typescript": "^1.0.4",
    "@oriflame/config-webpack": "^2.0.10",
    "@oriflame/conventional-changelog-lint-config": "^1.0.3",
    "@oriflame/lumos": "^2.0.16",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "typescript-utility-types": "^1.1.1"
  },
  "lumos": {
    "drivers": [
      "babel",
      "eslint",
      "jest",
      "prettier",
      "typescript",
      "webpack"
    ],
    "settings": {
      "react": true,
      "next": true
    }
  },
  "scripts": {
    "prepare": "lumos create-config --silent",
    "build": "cross-env NODE_ENV=production lumos webpack",
    "lint": "lumos eslint",
    "posttest": "yarn run lint",
    "jest": "cross-env NODE_ENV=test TZ=UTC lumos jest",
    "jest:coverage": "yarn run jest  --coverage",
    "test": "yarn run jest:coverage",
    "prettier": "lumos prettier",
    "type": "lumos typescript --noEmit",
    "pretest": "yarn run type",
    "start": "lumos create-config webpack --silent && lumos-webpack-server"
  },
  "dependencies": {
    "@ori-ui/components": "^1.0.2",
    "@ori-ui/icons": "^1.0.0",
    "@ori-ui/material-ui": "^1.0.1",
    "clsx": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
